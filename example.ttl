<https://example.com#859b22d6-d4a2-49f0-83c2-42c1eb49e35a> rdf:type rdfs:Resource;
    dcterms:identifier "859b22d6-d4a2-49f0-83c2-42c1eb49e35a";
    rdfs:label "Line Science";
    dcterms:description "considers the operational behavior of process flows consisting of logically connected processes separated by stockpoints. This focuses in particular on the issues that arise due to the coupling effects between processes in a flow";
    ex:status "draft";
